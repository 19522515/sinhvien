<body>
    <%- include('../part/header') %>
    <div class="container w-50 mt-5">
        <h1>Edit sinhvien mới</h1>
        <form action="/sinhvien/edit/<%= sinhvien._id %>?_method=PUT" method="POST">
             <div class="form-floating mb-3">
                <input type="text" class="form-control" value="<%= sinhvien.name %>" id="floatingInput" placeholder="sinhvien Name" name="name"> 
                <label for="floatingInput">Sinh vien Name</label>
              </div>
              <div class="form-floating">
                <input type="text" class="form-control" value="<%= sinhvien.name %>" id="floatingPassword" placeholder="Old" name="old"> 
                <label for="floatingPassword">Old</label>
              </div>
              <div class="form-floating mt-3">
                <input type="submit" class="form-control btn btn-primary" id="floatingPassword" value="Save">
              </div>
        </form>
    </div>
